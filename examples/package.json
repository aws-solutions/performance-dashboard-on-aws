{
    "name": "lambda",
    "version": "1.9.0",
    "description": "test project for lambda",
    "license": "Apache-2.0",
    "author": {
        "name": "Amazon Web Services",
        "url": "https://aws.amazon.com/solutions"
    },
    "main": "index.js",
    "scripts": {
        "test": "LOG_LEVEL=SILENT jest --coverage --watchAll",
        "test:ci": "jest --coverage --ci --passWithNoTests",
        "build": "npm run clean && tsc && npm run build:backend && npm run install:prod",
        "build:backend": "npm run build --prefix ../backend",
        "install:prod": "mkdir -p ./backend && cp -R ../backend/build/* ./backend/ && npm install --production --prefix build",
        "clean": "rm -rf build && rm -rf backend",
        "export": "ts-node src/export.ts && prettier --write resources",
        "import": "ts-node src/import.ts"
    },
    "devDependencies": {
        "@types/fs-extra": "^9.0.13",
        "@types/prompt": "^1.1.1",
        "@types/prompts": "^2.0.14",
        "prettier": "^2.4.1",
        "ts-loader": "^9.2.6",
        "@types/aws-lambda": "^8.10.57",
        "@types/aws-sdk": "^2.7.0",
        "@types/aws-serverless-express": "^3.3.3",
        "@types/cors": "^2.8.6",
        "@types/escape-html": "^1.0.2",
        "@types/express": "^4.17.6",
        "@types/jest": "^27.4.0",
        "@types/node": "^16.11.7",
        "@types/pino": "^6.3.4",
        "@types/uuid": "^8.0.0",
        "@typescript-eslint/eslint-plugin": "^5.45.1",
        "eslint": "^8.29.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard-with-typescript": "^23.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "jest": "^27.4.7",
        "lint-staged": "^13.1.0",
        "ts-jest": "^27.1.3",
        "ts-node": "^8.10.2",
        "typescript": "4.8.4"
    },
    "dependencies": {
        "envalid": "^7.2.2",
        "fs-extra": "^10.0.0",
        "performance-dashboard-backend": "file:../backend",
        "prompts": "^2.4.2",
        "yargs": "^17.2.1",
        "aws-sdk": "^2.1354.0",
        "aws-serverless-express": "^3.3.8",
        "aws-xray-sdk": "^3.1.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "jsonschema": "^1.4.0",
        "pino": "^6.7.0",
        "uuid": "^8.2.0"
    },
    "overrides": {
        "semver": "7.5.2",
        "word-wrap" : "npm:@aashutoshrathi/word-wrap@1.2.5"
    }
