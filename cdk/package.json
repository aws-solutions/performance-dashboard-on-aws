{
    "name": "performance-dashboard-cdk",
    "version": "1.9.0",
    "description": "Performance Dashboard on AWS CDK",
    "license": "Apache-2.0",
    "author": {
        "name": "Amazon Web Services",
        "url": "https://aws.amazon.com/solutions"
    },
    "bin": {
        "cdk": "bin/main.js"
    },
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "test": "jest --passWithNoTests",
        "cdk": "cdk",
        "deploy:pipeline": "cdk deploy --app 'npx ts-node bin/pipeline.ts'",
        "deploy:github": "cdk deploy --app 'npx ts-node bin/github.ts'",
        "lint-src": "lint-staged"
    },
    "lint-staged": {
        "*.{js,ts,tsx, jsx}": [
            "eslint --quiet --fix"
        ],
        "*.{json,md,html,js,jsx,ts,tsx}": [
            "prettier --write"
        ]
    },
    "devDependencies": {
        "@types/aws-sdk": "^2.7.0",
        "@types/node": "^16.11.7",
        "@typescript-eslint/eslint-plugin": "^5.45.1",
        "aws-cdk": "^2.86.0",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard-with-typescript": "^23.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.31.11",
        "lint-staged": "^13.1.0",
        "ts-node": "10.9.1",
        "typescript": "4.8.4"
    },
    "dependencies": {
        "@aws-cdk/aws-servicecatalogappregistry-alpha": "^2.60.0-alpha.0",
        "aws-cdk-lib": "^2.86.0",
        "aws-sdk": "^2.1270.0",
        "cdk-nag": "^2.21.43",
        "constructs": "^10.1.196",
        "source-map-support": "^0.5.21"
    },
    "overrides": {
        "semver": "7.5.2"
    }
}
