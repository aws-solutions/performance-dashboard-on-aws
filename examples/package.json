{
  "name": "lambda",
  "version": "1.0.0",
  "description": "test project for lambda",
  "main": "index.js",
  "scripts": {
    "run": "build/cli.js",
    "test": "LOG_LEVEL=SILENT jest --coverage --watchAll",
    "test:ci": "jest --coverage --ci",
    "build": "npm run clean && tsc && npm run build:backend && npm run install:prod",
    "build:backend": "npm run build --prefix ../backend",
    "install:prod": "npm install --production --prefix build && rm -rf backend/* && cp -R ../backend/build/* backend/",
    "clean": "rm -rf build && rm -rf backend",
    "export": "ts-node src/cli.ts --operation export && prettier --write resources",
    "import": "ts-node src/cli.ts --operation import"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.57",
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^27.0.2",
    "@types/prompt": "^1.1.1",
    "@types/uuid": "^8.3.1",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "aws-sdk": "^2.1021.0",
    "envalid": "^7.2.2",
    "fs-extra": "^10.0.0",
    "optimist": "^0.6.1",
    "performance-dashboard-backend": "file:../backend",
    "prompt": "^1.2.0",
    "uuid": "^8.3.2"
  }
}
