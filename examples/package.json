{
  "name": "lambda",
  "version": "1.0.0",
  "description": "test project for lambda",
  "main": "index.js",
  "scripts": {
    "test": "LOG_LEVEL=SILENT jest --coverage --watchAll",
    "test:ci": "jest --coverage --ci --passWithNoTests",
    "build": "npm run clean && tsc && npm run build:backend && npm run install:prod",
    "build:backend": "npm run build --prefix ../backend",
    "install:prod": "npm install --production --prefix build && mkdir -p ./build/backend && cp -R ../backend/build/* ./build/backend/ && rm ./build/node_modules/performance-dashboard-backend && ln -s $(pwd)/build/backend ./build/node_modules/performance-dashboard-backend",
    "clean": "rm -rf build && rm -rf backend",
    "export": "ts-node src/export.ts && prettier --write resources",
    "import": "ts-node src/import.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.57",
    "@types/aws-sdk": "^2.7.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@types/prompt": "^1.1.1",
    "@types/prompts": "^2.0.14",
    "@types/uuid": "^8.3.1",
    "aws-sdk": "^2.1055.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "envalid": "^7.2.2",
    "fs-extra": "^10.0.0",
    "performance-dashboard-backend": "file:../backend",
    "prompts": "^2.4.2",
    "uuid": "^8.3.2",
    "yargs": "^17.2.1"
  }
}
